    "@types/nprogress": "^0.2.3",
    "@types/react": "^16.14.62",
    "@types/react-dom": "^16.9.25",
    "@types/react-loadable": "^5.5.11",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^4.24.16",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "flyio": "^0.6.14",
    "loadsh": "^0.0.4",
    "marked": "^1.2.9",
    "nprogress": "^0.2.0",
    "react-hot-loader": "^4.13.1",
    "react-loadable": "^5.5.0",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1"
